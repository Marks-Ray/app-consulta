<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/tv_idC"
        android:visibility="invisible"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="ID medico:"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:id="@+id/tv_idmedicoC"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="ID Paciente:"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:id="@+id/tv_idpacienteC"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="data hora inicio:"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:id="@+id/tv_dataInicio"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Data hora final:"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:id="@+id/tv_dataFinal"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Observacao:"/>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:id="@+id/tv_obs"/>
</LinearLayout>
